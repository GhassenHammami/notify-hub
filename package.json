{
  "name": "notify-hub",
  "version": "1.0.0",
  "description": "The complete notification solution for developers. Send emails, SMS, and push notifications with powerful APIs and beautiful templates.",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Ghassen Hammami",
  "repository": {
    "type": "git",
    "url": "https://github.com/GhassenHammami/notify-hub.git"
  },
  "homepage": "https://github.com/GhassenHammami/notify-hub#readme",
  "keywords": [
    "notifications",
    "email",
    "sms",
    "push-notifications",
    "adonisjs",
    "react",
    "inertia",
    "typescript",
    "tailwindcss"
  ],
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#utils/*": "./app/utils/*.js",
    "#enums/*": "./app/enums/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
