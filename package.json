    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "^1.13.4",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "ts-node-maintained": "^10.9.5",
    "typescript": "^5.9.2",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.2",
    "@adonisjs/core": "^6.18.0",
