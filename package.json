    "nanoid": "^5.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-react": "^5.29.0",
    "tailwindcss": "^4.1.11",
    "yup": "^1.7.0"
